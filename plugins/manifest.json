{
    "manifest_version": 3,
    "name": "twitter_dfs_spider_pluging",
    "description": "ins/fb/tw",
    "version": "1.0",
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup/popup.html"
    },
    "background": {
        // "service_worker": "background/background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "twitter/twitter.js",
                "ins/ins.js"
            ]
        }
    ],
    "permissions": [
        "tabs",
        "activeTab",
        "scripting",
        "tabGroups",
        "storage",
        "unlimitedStorage",
        "cookies"
    ],
    "host_permissions": [
        "https://www.google.com/",
        "http://localhost:3000/"
    ]
}